{
  "name": "wiggles",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.11.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "exif-js": "^2.3.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^2.0.9",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.0.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-react-hook": "^3.3.2",
    "tinycolor2": "^1.4.1",
    "typescript": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint ./ --fix",
      "git add"
    ]
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/tailwind.css",
    "watch:css": "postcss src/styles/index.css -o src/tailwind.css -w",
    "start": "yarn run build:css && cross-env BROWSER=none react-app-rewired start",
    "build": "yarn run build:css && react-app-rewired build",
    "test": "react-app-rewired test --verbose=false",
    "eject": "react-app-rewired eject",
    "lint": "eslint ./ --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-plugin-root-import": "^6.1.0",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.12",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.1",
    "tailwindcss": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
