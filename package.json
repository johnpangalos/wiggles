{
  "name": "wiggles-pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run -r dev",
    "lint": "pnpm run -r lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.5",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "pnpm lint"
    ]
  }
}
